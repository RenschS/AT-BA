
<%= form_for @analytic do |f| %>
    <% if @analytic.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@analytic.errors.count, "error") %>Analyse konnte nicht durchgeführt
            werden aus folgenden Gründen:</h2>
          <ul><% @analytic.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %></ul>
        </div>
    <% end %>


    <table>
      <tr>
        <th><p>Geben sie das zu analysierende Event, den Zeitraum der Erfassung und die entsprechenden ApiKeys ein.</p></th>
      </tr>
    </table>
    <table>
    <tr>

            <td><p>

              <%= f.label :Eventname %><br>

              <%= f.text_field :title %>
            </p>
          </td>
          </tr>

          <tr>
            <td><p>
                <%= f.label :Startdatum %> (YYYY-MM-DD)<br>
                <%= f.text_field :date1 %><br>
            </p>
            </td>
            <td><p>
                <%= f.label :Enddatum %> (YYYY-MM-DD)<br>
                <%= f.text_field :date2 %><br>
            </p>
            </td>
          </tr>

            <tr>
            <td><p>
              <%= f.label :AndroidKey %> (20 Stellig)<br>
              <%= f.text_field :android_key %><br>
            </p>
            </td>
            <td><p>
              <%= f.label :IphoneKey %> (20 Stellig)<br>
              <%= f.text_field :iphone_key %><br>
            </p>
            </td>
            <td><p>
              <%= f.label :IpadKey %> (20 Stellig)<br>
              <%= f.text_field :ipad_key %><br>
            </p>
            </td>
          </tr>
    </table>

    <p>
      <br>
      <%= f.submit %>
      <br>
      <br>
    </p>


<% end %>